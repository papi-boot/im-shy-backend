{
  "name": "imshy-backend",
  "version": "1.0.0",
  "description": "im shy backend service",
  "main": "main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "postinstall": "npm run build",
    "dev": "nodemon main.ts",
    "db:migrate:dev": "npx sequelize-cli --env development db:migrate",
    "db:migrate:dev:undo": "npx sequelize-cli --env development db:migrate:undo",
    "db:migrate:prod": "npx sequelize-cli --env production db:migrate",
    "db:migrate:prod:undo": "npx sequelize-cli --env production db:migrate:undo"
  },
  "keywords": [
    "imshy"
  ],
  "author": "Jason Evangelista",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-session-sequelize": "^7.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dompurify": "^2.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^6.0.0",
    "express-session": "^1.17.2",
    "jsdom": "^18.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "sequelize": "^6.7.0",
    "socket.io": "^4.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dompurify": "^2.3.1",
    "@types/express": "^4.17.13",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/express-session": "^1.17.4",
    "@types/jsdom": "^16.2.13",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "sequelize-cli": "^6.3.0",
    "typescript": "^4.5.2"
  }
}
